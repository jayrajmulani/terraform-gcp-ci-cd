#jinja2: lstrip_blocks: "True"

global:
  scrape_interval: "5s"
  evaluation_interval: 5s
scrape_configs:
{% if webservers %}
  - job_name: webservers
    scrape_interval: 5s
      static_configs:
      - targets:
      {% for server in groups[webservers] %}
        - {{ server }}:80
      {% endfor %}
{% endif %}